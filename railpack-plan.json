{
  "install": {
    "cmd": "pnpm install --prefer-offline --filter backend --include=dev",
    "shell": "sh"
  },
  "build": {
    "cmd": "pnpm --filter backend build",
    "shell": "sh"
  },
  "start": {
    "cmd": "node build/index.js",
    "shell": "sh",
    "workingDir": "backend"
  },
  "packageManager": "pnpm"
}
